{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default"
    ],
    "sharedGlobals": []
  },
  "generators": {
    "@nx/js:library": {
      "tags": "lib:js",
      "projectNameAndRootFormat": "as-provided",
      "unitTestRunner": "jest",
      "testEnvironment": "node",
      "linter": "eslint",
      "bundler": "tsc",
      "config": "project",
      "strict": true,
      "publishable": false,
      "includeBabelRc": false,
      "minimal": false,
      "simpleName": false,
      "importPath": ""
    },
    "@nx/node:application": {
      "tags": "app:backend",
      "projectNameAndRootFormat": "as-provided",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "linter": "eslint",
      "bundler": "webpack",
      "framework": "express",
      "swcJest": false,
      "docker": true,
      "isNest": false,
      "frontendProject": "",
      "port": -1
    },
    "@nx/node:library": {
      "tags": "lib:backend",
      "projectNameAndRootFormat": "as-provided",
      "unitTestRunner": "jest",
      "testEnvironment": "node",
      "linter": "eslint",
      "compiler": "tsc",
      "strict": true,
      "buildable": true,
      "publishable": false,
      "simpleModuleName": false,
      "babelJest": false,
      "importPath": ""
    },
    "@nx/angular:application": {
      "tags": "app:frontend",
      "projectNameAndRootFormat": "as-provided",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "linter": "eslint",
      "bundler": "esbuild",
      "strict": true,
      "prefix": "lego",
      "style": "scss",
      "ssr": true,
      "routing": true,
      "standalone": true,
      "inlineStyle": true,
      "inlineTemplate": true,
      "viewEncapsulation": "ShadowDom",
      "minimal": false,
      "backendProject": "",
      "port": -1
    },
    "@nx/angular:library": {
      "tags": "lib:frontend",
      "projectNameAndRootFormat": "as-provided",
      "unitTestRunner": "jest",
      "linter": "eslint",
      "strict": true,
      "buildable": true,
      "publishable": false,
      "compilationMode": "full",
      "prefix": "lego",
      "style": "scss",
      "lazy": false,
      "parent": "_untitled_lazy_parent_",
      "routing": false,
      "standalone": true,
      "skipTests": false,
      "skipModule": false,
      "skipSelector": false,
      "displayBlock": false,
      "inlineStyle": true,
      "inlineTemplate": true,
      "changeDetection": "OnPush",
      "viewEncapsulation": "ShadowDom",
      "flat": false,
      "simpleName": true,
      "addModuleSpec": false,
      "importPath": ""
    },
    "@nx/angular:component": {
      "nameAndDirectoryFormat": "as-provided",
      "prefix": "lego",
      "style": "scss",
      "standalone": true,
      "displayBlock": false,
      "inlineStyle": true,
      "inlineTemplate": true,
      "changeDetection": "OnPush",
      "viewEncapsulation": "ShadowDom",
      "skipTests": "",
      "skipImport": "",
      "skipSelector": false,
      "export": false,
      "flat": false,
      "type": "component",
      "module": "_untitled_module_"
    },
    "@nx/angular:directive": {
      "nameAndDirectoryFormat": "as-provided",
      "project": "_deprecated_",
      "standalone": true,
      "skipTests": false,
      "skipImport": true,
      "export": false,
      "flat": false,
      "module": "_untitled_module_"
    }
  }
}
